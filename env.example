# Next.js
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
OPENAI_API_KEY=
NEXT_PUBLIC_DEFAULT_USER_ID=

# Image provider (Replicate only)
MAMETTE_IMAGE_PROVIDER=replicate

# Replicate configuration (base text-to-image for cover generation)
REPLICATE_API_TOKEN=
REPLICATE_MODEL=bytedance/seedream-4
REPLICATE_VERSION=

# Replicate image-conditioned model for mockups (IP-Adapter/ControlNet style)
# Examples (set one you have access to):
# tencentarc/photomaker, cfvae/ip-adapter, stability-ai/sdxl-controlnet, etc.
REPLICATE_MOCKUP_MODEL=
REPLICATE_MOCKUP_VERSION=

# OCR toggle (enable to filter text-containing images)
MAMETTE_OCR_ENABLED=true

# Save exported images locally (public/exports). Useful in local dev.
MAMETTE_SAVE_EXPORTS=true 