# Next.js
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
OPENAI_API_KEY=
NEXT_PUBLIC_DEFAULT_USER_ID=

# Image provider (Replicate only)
MAMETTE_IMAGE_PROVIDER=replicate

# Replicate configuration
REPLICATE_API_TOKEN=
# Provide either a model slug (owner/model) to auto-resolve latest version,
# or set a specific version id. Example working models:
# bytedance/seedream-4 (high quality image generation)
# stability-ai/sdxl (stable, reliable)
REPLICATE_MODEL=bytedance/seedream-4
REPLICATE_VERSION=

# OCR toggle (enable to filter text-containing images)
MAMETTE_OCR_ENABLED=true 