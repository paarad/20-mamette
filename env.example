# Next.js
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
OPENAI_API_KEY=
NEXT_PUBLIC_DEFAULT_USER_ID=

# Image provider: 'dalle' (default) or 'replicate'
MAMETTE_IMAGE_PROVIDER=dalle

# Replicate (set when using provider=replicate)
REPLICATE_API_TOKEN=
# Either provide a model slug (owner/model) to auto-resolve latest version, or set a specific version id
REPLICATE_MODEL=bananaml/nano-nano
REPLICATE_VERSION=

# OCR toggle
MAMETTE_OCR_ENABLED=true 